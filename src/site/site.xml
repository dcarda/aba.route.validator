<?xml 
    version="1.0" 
    encoding="UTF-8"?>

<project 
    xmlns="http://maven.apache.org/DECORATION/1.3.0" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    name="ABA Routing Number Validator" 
    xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd">

    <!-- Reference the Reflow Skin   ========================================= -->
    <skin>
        <groupId>lt.velykis.maven.skins</groupId>
        <artifactId>reflow-maven-skin</artifactId>
        <version>1.1.1</version>
    </skin>
    <custom>

        <!-- =============== Global skin configuration ================= -->
        <reflowSkin>

            <!--  This will set the color theme 
            <theme>bootswatch-superhero</theme>  -->  
            <theme>bootswatch-simplex</theme>  

            <!-- Comment this out for local development. 
            <absoluteResourceURL>https://grizzly.java.net<;/absoluteResourceURL> -->

            <bootstrapIcons>true</bootstrapIcons>

            <!-- Brand on the top menu bar -->
            <brand>
                <name>ABA Routing Number Validator Project</name>
                <href>https://github.com/dcarda/aba.route.validator</href>
            </brand>

            <!-- Slogan appears under the Grizzly Head logo -->
            <slogan>Super interesting project doing good things.</slogan>

            <!-- Syntax highlighting Configuration -->
            <highlightJs>true</highlightJs>
            <highlightJsTheme>solarized_dark</highlightJsTheme>

            <!--  Navigation Menus At Top Of All Pages  -->
            <topNav>Download|reports</topNav>

            <!--  Navigation Menus At Bottom Of All Pages  -->
            <bottomNav>
                <column>Project Links|Download|Donations / Pricing|Maven Theme</column>
                <column>Developer Reports</column>
                <column>Project Information</column>
            </bottomNav>
            <bottomDescription>This is a very good project doing interesting and valuable things.</bottomDescription>

            <!-- Table of Contents - displayed on all pages unless explicitly
            disabled on a per-page basis. -->
            <toc>top</toc>
            <tocTopMax>4</tocTopMax>

            <!-- Misc Configuration -->
            <smoothScroll>true</smoothScroll>
            <skinAttribution>false</skinAttribution>

            <!-- Per-Page Configuration (allows overriding of global) -->
            <pages>
                <index project="project-id">
                    <shortTitle>Welcome</shortTitle>
                    <breadcrumbs>false</breadcrumbs>
                    <toc>false</toc>
                    <titleTemplate>%2$s | %1$s</titleTemplate>

                    <sections>
                        <carousel />
                        <body />
                        <sidebar />
                        <thumbs>2</thumbs>
                        <columns>3</columns>
                    </sections>
                </index>
                <developer-info>
                    <toc>sidebar</toc>
                </developer-info>

                <!-- Disable source highlighting for Maven reports -->
                <source-repository>
                  <highlightJs>false</highlightJs>
                </source-repository>
                <issue-tracking>
                  <highlightJs>false</highlightJs>
                  <toc>false</toc>
                </issue-tracking>
                <license>
                  <highlightJs>false</highlightJs>
                  <toc>false</toc>
                </license>
                <!-- Disable ToC for some Maven reports -->
                <project-info>
                  <toc>false</toc>
                </project-info>
                <github-report>
                  <toc>false</toc>
                </github-report>
                <dependencies>
                  <tocTopMax>3</tocTopMax>
                </dependencies>



            </pages>
        </reflowSkin>
    </custom>

    <!--    -->
